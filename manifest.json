{
    "name": "TAN",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action":{
        "default_popup": "GUI.html",
        "default_title": "TAN"
    }
}